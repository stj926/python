def a(func):
    print(1)
    def b(*args, **kwargs):
      print(2)
      func(*args, **kwargs)
      print(3)
    return b
    
@a
def v(a, b):
  print(a+b)

c(3,4)

输出结果：
1
2
7
3


# @property
class Demo(object):
    def __init__(self):
        self.name = 'stj'
        self.__age = 18
        self._other = 'yes'

    @property
    def agefunc(self):
        return self.__age

    @agefunc.setter
    def agefunc(self, getage):
        self.__age = getage
